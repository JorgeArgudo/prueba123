<header>
    <!-- Logo to helloWorld -->
    <a href="/helloWorld">
        <h1>
            <img class="img-fluid logo2" src={{content.fields.logo.fields.file.url}}
            alt="{{content.fields.logo.fields.description}}" />
         </h1>
    </a>

    <!-- Navigation bar -->
    <nav class="row nav justify-content-center">
        <ul class="justify-content-center" id="nav">
            {{#each content.fields.navBar}}
            <li class="justify-content-center"><a href={{this.fields.href}} class="nav-link {{this.fields.color}}">{{this.fields.text}}</a>
            <span class="down-arrow" aria-hidden="true"></span>
            </li>
            {{/each}}
        </ul>
    </nav>
</header>

<!-- Carousel -->
<div id="carousel" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#carousel" data-slide-to="0" class="active"></li>
        <li data-target="#carousel" data-slide-to="1"></li>
        <li data-target="#carousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src={{content.fields.carousel.0.fields.file.url}}
             alt="{{content.fields.carousel.0.fields.description}}">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src={{content.fields.carousel.1.fields.file.url}}
             alt="{{content.fields.carousel.1.fields.description}}">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src={{content.fields.carousel.2.fields.file.url}}
             alt="{{content.fields.carousel.2.fields.description}}">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

